#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i=1, n, ans=0;
    printf ("n=");
    scanf("%d", &n);
    while (i<n)
    {
        int j=1;
        while (j<n)
        {
            if (n==i*i*i+j*j*j)
            {
                ans=ans+1;
            }
            j++;
        }
        i++;
    }
    printf ("%d", ans/2);
    return 0;
}
